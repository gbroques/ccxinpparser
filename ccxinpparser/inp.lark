start: keyword_card+
     | // empty
keyword_card: KEYWORD ("," param)*
KEYWORD: "*" WORD
param: KEY "=" value
     | KEY
KEY: WORD
value: PATH
PATH: /.+\/[^\/]+/

SINGLE_LINE_COMMENT: "**" [/(^NEWLINE)+/]

%import common.WORD
%import common.NEWLINE
%import common.WS
%ignore WS
%ignore SINGLE_LINE_COMMENT
